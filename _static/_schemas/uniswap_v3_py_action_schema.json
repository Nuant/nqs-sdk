{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PyUniswapV3Action",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "args",
        "name"
      ],
      "properties": {
        "args": {
          "type": "object",
          "anyOf": [
            {
              "type": "object",
              "required": [
                "tick_lower",
                "tick_upper"
              ],
              "properties": {
                "tick_lower": {
                  "$ref": "#/definitions/SimInt24"
                },
                "tick_upper": {
                  "$ref": "#/definitions/SimInt24"
                }
              }
            },
            {
              "type": "object",
              "required": [
                "price_lower",
                "price_upper"
              ],
              "properties": {
                "price_lower": {
                  "$ref": "#/definitions/Amount"
                },
                "price_upper": {
                  "$ref": "#/definitions/Amount"
                }
              }
            }
          ],
          "properties": {
            "from": {
              "description": "override address of sender, useful with agents. if the wallet does not exist it has infinite liquidity",
              "anyOf": [
                {
                  "$ref": "#/definitions/EthAddr"
                },
                {
                  "type": "null"
                }
              ]
            },
            "token_id": {
              "description": "the label of the position, used to query the position in uniswap_v3_agent_metrics",
              "anyOf": [
                {
                  "$ref": "#/definitions/ArcStr"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        },
        "name": {
          "type": "string",
          "enum": [
            "mint"
          ]
        }
      }
    },
    {
      "type": "object",
      "required": [
        "args",
        "name"
      ],
      "properties": {
        "args": {
          "type": "object",
          "anyOf": [
            {
              "type": "object",
              "required": [
                "tick_lower",
                "tick_upper"
              ],
              "properties": {
                "tick_lower": {
                  "$ref": "#/definitions/SimInt24"
                },
                "tick_upper": {
                  "$ref": "#/definitions/SimInt24"
                }
              }
            },
            {
              "type": "object",
              "required": [
                "price_lower",
                "price_upper"
              ],
              "properties": {
                "price_lower": {
                  "$ref": "#/definitions/Amount"
                },
                "price_upper": {
                  "$ref": "#/definitions/Amount"
                }
              }
            }
          ],
          "properties": {
            "from": {
              "description": "override address of sender, useful with agents. if the wallet does not exist it has infinite liquidity",
              "anyOf": [
                {
                  "$ref": "#/definitions/EthAddr"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        },
        "name": {
          "type": "string",
          "enum": [
            "burn"
          ]
        }
      }
    },
    {
      "type": "object",
      "required": [
        "args",
        "name"
      ],
      "properties": {
        "args": {
          "type": "object",
          "anyOf": [
            {
              "type": "object",
              "required": [
                "amount0_in"
              ],
              "properties": {
                "amount0_in": {
                  "$ref": "#/definitions/Amount"
                }
              }
            },
            {
              "type": "object",
              "required": [
                "amount1_in"
              ],
              "properties": {
                "amount1_in": {
                  "$ref": "#/definitions/Amount"
                }
              }
            },
            {
              "type": "object",
              "required": [
                "amount0_out"
              ],
              "properties": {
                "amount0_out": {
                  "$ref": "#/definitions/Amount"
                }
              }
            },
            {
              "type": "object",
              "required": [
                "amount1_out"
              ],
              "properties": {
                "amount1_out": {
                  "$ref": "#/definitions/Amount"
                }
              }
            }
          ],
          "properties": {
            "from": {
              "description": "override address of sender, useful with agents. if the wallet does not exist it has infinite liquidity",
              "anyOf": [
                {
                  "$ref": "#/definitions/EthAddr"
                },
                {
                  "type": "null"
                }
              ]
            },
            "price_limit": {
              "$ref": "#/definitions/Amount"
            },
            "sqrt_price_limit_x96": {
              "$ref": "#/definitions/SimUint160"
            }
          }
        },
        "name": {
          "type": "string",
          "enum": [
            "swap"
          ]
        }
      }
    },
    {
      "type": "object",
      "required": [
        "args",
        "name"
      ],
      "properties": {
        "args": {
          "type": "object",
          "anyOf": [
            {
              "type": "object",
              "required": [
                "tick_lower",
                "tick_upper"
              ],
              "properties": {
                "tick_lower": {
                  "$ref": "#/definitions/SimInt24"
                },
                "tick_upper": {
                  "$ref": "#/definitions/SimInt24"
                }
              }
            },
            {
              "type": "object",
              "required": [
                "price_lower",
                "price_upper"
              ],
              "properties": {
                "price_lower": {
                  "$ref": "#/definitions/Amount"
                },
                "price_upper": {
                  "$ref": "#/definitions/Amount"
                }
              }
            }
          ],
          "properties": {
            "from": {
              "description": "override address of sender, useful with agents. if the wallet does not exist it has infinite liquidity",
              "anyOf": [
                {
                  "$ref": "#/definitions/EthAddr"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        },
        "name": {
          "type": "string",
          "enum": [
            "collect"
          ]
        }
      }
    }
  ],
  "definitions": {
    "Amount": {
      "$ref": "#/definitions/Decimal"
    },
    "AmountRatio": {
      "$ref": "#/definitions/Decimal"
    },
    "ArcStr": {
      "description": "[ArcStr] allows us to have immutable str that can be cheaply cloned and possibly cheaply compared if they have the same pointer.",
      "type": "string"
    },
    "Decimal": {
      "oneOf": [
        {
          "type": "number"
        },
        {
          "type": "string",
          "pattern": "^-?[0-9][0-9_]*(\\.[0-9][0-9_]*)?((e|E)-?[0-9][0-9_]*)?"
        }
      ]
    },
    "EthAddr": {
      "type": "string"
    },
    "SimInt24": {
      "oneOf": [
        {
          "type": "integer"
        },
        {
          "type": "string",
          "pattern": "^-?[0-9][0-9_]*((e|E)[0-9][0-9_]*)?$"
        }
      ]
    },
    "SimUint160": {
      "oneOf": [
        {
          "type": "number",
          "minimum": 0.0
        },
        {
          "type": "string",
          "pattern": "^(0x[0-9a-fA-F_]+|[0-9][0-9_]*((e|E)[0-9][0-9_]*)?)$"
        }
      ],
      "minimum": 0.0
    }
  }
}